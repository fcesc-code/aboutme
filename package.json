{
  "name": "aboutme",
  "version": "0.0.1",
  "private": true,
  "description": "Personal site project created from uoc-boilerplate",
  "scripts": {
    "build": "npm-run-all test stylelint lint clean parcel:build",
    "clean": "rimraf dist .cache .cache-loader .parcel-cache",
    "dev": "npm-run-all clean parcel:serve",
    "lintfix": "eslint src/** --fix",
    "lint": "eslint src/**",
    "parcel:serve": "parcel src/index.html -p 8123 --target web --open",
    "parcel:build": "parcel build src/index.html --target web --no-source-maps --no-cache",
    "start": "npm-run-all parcel:serve",
    "stylelint": "stylelint src/assets/styles/**/*.sass --syntax sass",
    "stylelintfix": "stylelint src/assets/styles/**/*.sass --syntax sass --fix",
    "tcoverage": "jest --coverage",
    "tdev": "jest --watchAll --coverage",
    "test": "jest",
    "twatch": "jest --watchAll"
  },
  "web": "dist/index.html",
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "./"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fcesc-code/curriculum.git"
  },
  "author": {
    "name": "Francesc Brugarolas",
    "email": "fbrugarolasp@uoc.edu"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fcesc-code/curriculum/issues"
  },
  "homepage": "https://github.com/fcesc-code/curriculum/#readme",
  "keywords": [
    "Vanilla",
    "JS",
    "UOC",
    "curriculum"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@parcel/transformer-image": "2.8.3",
    "@parcel/transformer-sass": "2.8.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "parcel": "2.8.3",
    "postcss-preset-env": "^8.4.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1",
    "sharp": "^0.32.1",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1"
  },
  "stylelint": {
    "plugins": [
      "stylelint-order",
      "stylelint-scss",
      "stylelint-selector-bem-pattern"
    ],
    "rules": {
      "at-rule-no-unknown": true,
      "at-rule-no-vendor-prefix": true,
      "block-no-empty": true,
      "color-hex-case": "lower",
      "color-hex-length": "short",
      "color-no-invalid-hex": true,
      "comment-no-empty": null,
      "declaration-bang-space-after": "never",
      "declaration-bang-space-before": "always",
      "declaration-block-no-duplicate-custom-properties": true,
      "declaration-block-no-duplicate-properties": true,
      "declaration-block-single-line-max-declarations": 1,
      "declaration-property-value-disallowed-list": {
        "border": [
          "none"
        ],
        "border-top": [
          "none"
        ],
        "border-right": [
          "none"
        ],
        "border-bottom": [
          "none"
        ],
        "border-left": [
          "none"
        ]
      },
      "font-family-no-duplicate-names": true,
      "font-family-no-missing-generic-family-keyword": true,
      "keyframe-declaration-no-important": true,
      "length-zero-no-unit": true,
      "media-feature-name-no-vendor-prefix": true,
      "max-empty-lines": 2,
      "media-feature-name-no-unknown": true,
      "no-empty-first-line": true,
      "no-eol-whitespace": null,
      "no-irregular-whitespace": true,
      "number-leading-zero": null,
      "number-no-trailing-zeros": true,
      "order/properties-order": [
        "position",
        "inset",
        "top",
        "right",
        "bottom",
        "left",
        "z-index",
        "box-sizing",
        "display",
        "float",
        "flex",
        "flex-flow",
        "flex-direction",
        "flex-wrap",
        "flex-basis",
        "flex-grow",
        "flex-schrink",
        "grid-template",
        "grid-template-columns",
        "grid-template-rows",
        "grid-template-areas",
        "grid-column",
        "grid-column-start",
        "grid-column-end",
        "grid-row",
        "grid-row-start",
        "grid-row-end",
        "grid-area",
        "gap",
        "column-gap",
        "row-gap",
        "justify-items",
        "align-items",
        "justify-content",
        "align-content",
        "order",
        "place-self",
        "justify-self",
        "align-self",
        "width",
        "max-width",
        "min-width",
        "height",
        "max-height",
        "min-height",
        "margin",
        "margin-top",
        "margin-right",
        "margin-bottom",
        "margin-left",
        "padding",
        "padding-top",
        "padding-right",
        "padding-bottom",
        "padding-left",
        "overflow",
        "overflow-x",
        "overflow-y",
        "columns",
        "column-count",
        "column-fill",
        "column-gap",
        "column-rule",
        "column-rule-width",
        "column-rule-style",
        "column-rule-color",
        "column-span",
        "column-width",
        "orphans",
        "widows",
        "clip",
        "clear",
        "font",
        "font-familiy",
        "font-size",
        "font-style",
        "font-weight",
        "font-variant",
        "font-size-adjust",
        "font-stretch",
        "font-effect",
        "font-emphasize",
        "font-emphasize-position",
        "font-emphasize-style",
        "font-smooth",
        "word-break",
        "word-spacing",
        "word-wrap",
        "overflow-wrap",
        "tab-size",
        "letter-spacing",
        "white-space",
        "line-height",
        "color",
        "text-align",
        "vertical-align",
        "hyphens",
        "src",
        "direction",
        "unicode-bidi",
        "list-style",
        "list-style-position",
        "list-style-type",
        "list-style-image",
        "pointer-events",
        "-ms-touch-action",
        "touch-action",
        "cursor",
        "visibility",
        "zoom",
        "table-layout",
        "empty-cells",
        "caption-side",
        "border-spacing",
        "border-collapse",
        "content",
        "quotes",
        "counter-reset",
        "counter-increment",
        "resize",
        "user-select",
        "nav-index",
        "nav-up",
        "nav-right",
        "nav-down",
        "nav-left",
        "background",
        "background-color",
        "background-image",
        "filter",
        "background-repeat",
        "background-attachment",
        "background-position",
        "background-position-x",
        "background-position-y",
        "background-clip",
        "background-origin",
        "background-size",
        "border",
        "border-color",
        "border-style",
        "border-width",
        "border-top",
        "border-top-color",
        "border-top-style",
        "border-top-width",
        "border-right",
        "border-right-color",
        "border-right-style",
        "border-right-width",
        "border-bottom",
        "border-bottom-color",
        "border-bottom-style",
        "border-bottom-width",
        "border-left",
        "border-left-color",
        "border-left-style",
        "border-left-width",
        "border-radius",
        "border-top-left-radius",
        "border-top-right-radius",
        "border-bottom-right-radius",
        "border-bottom-left-radius",
        "border-image",
        "border-image-source",
        "border-image-slice",
        "border-image-width",
        "border-image-outset",
        "border-image-repeat",
        "outline",
        "outline-width",
        "outline-style",
        "outline-color",
        "outline-offset",
        "box-shadow",
        "opacity",
        "page-break-after",
        "page-break-before",
        "page-break-inside",
        "transition",
        "transition-delay",
        "transition-timing-function",
        "transition-duration",
        "transition-property",
        "transform",
        "transform-origin",
        "perspective",
        "appearance",
        "animation",
        "animation-name",
        "animation-duration",
        "animation-play-state",
        "animation-timing-function",
        "animation-delay",
        "animation-iteration-count",
        "animation-direction",
        "animation-fill-mode",
        "fill",
        "stroke"
      ],
      "plugin/selector-bem-pattern": {
        "componentName": "[A-Z]+",
        "componentSelectors": {
          "initial": "^\\.{componentName}(?:-[a-z]+)?$",
          "combined": "^\\.combined-{componentName}-[a-z]+$"
        },
        "utilitySelectors": "^\\.util-[a-z]+$"
      },
      "property-case": "lower",
      "property-no-unknown": true,
      "scss/comment-no-empty": true,
      "scss/double-slash-comment-whitespace-inside": "always",
      "scss/no-global-function-names": true,
      "scss/operator-no-newline-after": true,
      "scss/operator-no-newline-before": true,
      "scss/operator-no-unspaced": true,
      "scss/partial-no-import": true,
      "selector-no-vendor-prefix": true,
      "selector-pseudo-class-no-unknown": true,
      "selector-pseudo-element-no-unknown": true,
      "string-no-newline": true,
      "string-quotes": "single",
      "unit-case": "lower",
      "unit-no-unknown": true,
      "value-keyword-case": "lower",
      "value-list-comma-space-after": "always",
      "value-no-vendor-prefix": true
    }
  }
}
